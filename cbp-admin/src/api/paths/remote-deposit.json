{
  "/remote-deposit": {
    "get": {
      "tags": ["remote-deposit"],
      "summary": "List all remote deposit configurations",
      "operationId": "listRemoteDepositConfigs",
      "parameters": [
        {
          "name": "page",
          "in": "query",
          "schema": {
            "type": "integer",
            "default": 1
          }
        },
        {
          "name": "limit",
          "in": "query",
          "schema": {
            "type": "integer",
            "default": 20
          }
        },
        {
          "name": "provider",
          "in": "query",
          "schema": {
            "type": "string",
            "enum": ["ProfitStars", "Vertifi", "Ensenta"]
          }
        }
      ],
      "responses": {
        "200": {
          "description": "Remote deposit configurations retrieved successfully",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "items": {
                    "type": "array",
                    "items": {
                      "$ref": "../models/remote-deposit.json#/RemoteDepositSettings"
                    }
                  },
                  "total": {
                    "type": "integer"
                  },
                  "page": {
                    "type": "integer"
                  },
                  "limit": {
                    "type": "integer"
                  }
                }
              }
            }
          }
        }
      }
    },
    "post": {
      "tags": ["remote-deposit"],
      "summary": "Create a new remote deposit configuration",
      "operationId": "createRemoteDepositConfig",
      "requestBody": {
        "required": true,
        "content": {
          "application/json": {
            "schema": {
              "$ref": "../models/remote-deposit.json#/RemoteDepositSettings"
            }
          }
        }
      },
      "responses": {
        "201": {
          "description": "Remote deposit configuration created successfully",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "../models/remote-deposit.json#/RemoteDepositSettings"
              }
            }
          }
        }
      }
    }
  },
  "/remote-deposit/{id}": {
    "get": {
      "tags": ["remote-deposit"],
      "summary": "Get a specific remote deposit configuration",
      "operationId": "getRemoteDepositConfig",
      "parameters": [
        {
          "name": "id",
          "in": "path",
          "required": true,
          "schema": {
            "type": "string",
            "format": "uuid"
          }
        }
      ],
      "responses": {
        "200": {
          "description": "Remote deposit configuration retrieved successfully",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "../models/remote-deposit.json#/RemoteDepositSettings"
              }
            }
          }
        }
      }
    },
    "put": {
      "tags": ["remote-deposit"],
      "summary": "Update a remote deposit configuration",
      "operationId": "updateRemoteDepositConfig",
      "parameters": [
        {
          "name": "id",
          "in": "path",
          "required": true,
          "schema": {
            "type": "string",
            "format": "uuid"
          }
        }
      ],
      "requestBody": {
        "required": true,
        "content": {
          "application/json": {
            "schema": {
              "$ref": "../models/remote-deposit.json#/RemoteDepositSettings"
            }
          }
        }
      },
      "responses": {
        "200": {
          "description": "Remote deposit configuration updated successfully",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "../models/remote-deposit.json#/RemoteDepositSettings"
              }
            }
          }
        }
      }
    },
    "delete": {
      "tags": ["remote-deposit"],
      "summary": "Delete a remote deposit configuration",
      "operationId": "deleteRemoteDepositConfig",
      "parameters": [
        {
          "name": "id",
          "in": "path",
          "required": true,
          "schema": {
            "type": "string",
            "format": "uuid"
          }
        }
      ],
      "responses": {
        "204": {
          "description": "Remote deposit configuration deleted successfully"
        }
      }
    }
  }
}
