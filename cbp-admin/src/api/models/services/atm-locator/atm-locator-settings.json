{
  "AtmLocatorSettings": {
    "type": "object",
    "description": "ATM Locator settings",
    "x-legacy-model": "Psi.Data.Models.ClientConfigurationModels.AtmLocator.AtmLocator",
    "properties": {
      "enabled": {
        "type": "boolean",
        "description": "Whether ATM Locator is enabled",
        "x-setting-key": "AtmLocator.Enabled"
      },
      "url": {
        "type": "string",
        "description": "URL for ATM Locator service",
        "x-setting-key": "AtmLocator.Url"
      },
      "minVersions": {
        "type": "object",
        "description": "Minimum version requirements for ATM Locator",
        "properties": {
          "android": {
            "type": "string",
            "description": "Minimum Android version required",
            "x-setting-key": "AtmLocator.MinAndroidVersion"
          },
          "ios": {
            "type": "string",
            "description": "Minimum iOS version required",
            "x-setting-key": "AtmLocator.MinIosVersion"
          }
        }
      }
    }
  }
}
