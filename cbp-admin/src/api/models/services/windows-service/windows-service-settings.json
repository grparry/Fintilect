                "isScheduled": {
                  "type": "boolean"
                },
                "name": {
                  "type": "string"
                }
              }
            }
          },
          "dataFeedProcessing": {
            "type": "object",
            "description": "Data feed processing configuration",
            "x-setting-key": "PsiServices.DataFeedProcessing.Configuration",
