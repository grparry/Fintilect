{
  "/audit-logging": {
    "get": {
      "tags": ["audit-logging"],
      "summary": "List all audit logging configurations",
      "operationId": "listAuditLoggingConfigs",
      "parameters": [
        {
          "name": "page",
          "in": "query",
          "schema": {
            "type": "integer",
            "default": 1
          }
        },
        {
          "name": "limit",
          "in": "query",
          "schema": {
            "type": "integer",
            "default": 20
          }
        },
        {
          "name": "provider",
          "in": "query",
          "schema": {
            "type": "string",
            "enum": ["ElasticSearch", "Splunk", "AzureMonitor", "Custom"]
          }
        }
      ],
      "responses": {
        "200": {
          "description": "Audit logging configurations retrieved successfully",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "items": {
                    "type": "array",
                    "items": {
                      "$ref": "../models/audit-logging.json#/AuditLoggingSettings"
                    }
                  },
                  "total": {
                    "type": "integer"
                  },
                  "page": {
                    "type": "integer"
                  },
                  "limit": {
                    "type": "integer"
                  }
                }
              }
            }
          }
        }
      }
    },
    "post": {
      "tags": ["audit-logging"],
      "summary": "Create a new audit logging configuration",
      "operationId": "createAuditLoggingConfig",
      "requestBody": {
        "required": true,
        "content": {
          "application/json": {
            "schema": {
              "$ref": "../models/audit-logging.json#/AuditLoggingSettings"
            }
          }
        }
      },
      "responses": {
        "201": {
          "description": "Audit logging configuration created successfully",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "../models/audit-logging.json#/AuditLoggingSettings"
              }
            }
          }
        }
      }
    }
  },
  "/audit-logging/{id}": {
    "get": {
      "tags": ["audit-logging"],
      "summary": "Get a specific audit logging configuration",
      "operationId": "getAuditLoggingConfig",
      "parameters": [
        {
          "name": "id",
          "in": "path",
          "required": true,
          "schema": {
            "type": "string",
            "format": "uuid"
          }
        }
      ],
      "responses": {
        "200": {
          "description": "Audit logging configuration retrieved successfully",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "../models/audit-logging.json#/AuditLoggingSettings"
              }
            }
          }
        }
      }
    },
    "put": {
      "tags": ["audit-logging"],
      "summary": "Update an audit logging configuration",
      "operationId": "updateAuditLoggingConfig",
      "parameters": [
        {
          "name": "id",
          "in": "path",
          "required": true,
          "schema": {
            "type": "string",
            "format": "uuid"
          }
        }
      ],
      "requestBody": {
        "required": true,
        "content": {
          "application/json": {
            "schema": {
              "$ref": "../models/audit-logging.json#/AuditLoggingSettings"
            }
          }
        }
      },
      "responses": {
        "200": {
          "description": "Audit logging configuration updated successfully",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "../models/audit-logging.json#/AuditLoggingSettings"
              }
            }
          }
        }
      }
    },
    "delete": {
      "tags": ["audit-logging"],
      "summary": "Delete an audit logging configuration",
      "operationId": "deleteAuditLoggingConfig",
      "parameters": [
        {
          "name": "id",
          "in": "path",
          "required": true,
          "schema": {
            "type": "string",
            "format": "uuid"
          }
        }
      ],
      "responses": {
        "204": {
          "description": "Audit logging configuration deleted successfully"
        }
      }
    }
  }
}
