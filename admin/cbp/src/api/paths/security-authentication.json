{
  "/security-authentication": {
    "get": {
      "tags": ["security-authentication"],
      "summary": "List all security and authentication configurations",
      "operationId": "listSecurityConfigs",
      "parameters": [
        {
          "name": "page",
          "in": "query",
          "schema": {
            "type": "integer",
            "default": 1
          }
        },
        {
          "name": "limit",
          "in": "query",
          "schema": {
            "type": "integer",
            "default": 20
          }
        },
        {
          "name": "feature",
          "in": "query",
          "schema": {
            "type": "string",
            "enum": [
              "passwordPolicy",
              "mfa",
              "deviceManagement",
              "sessionManagement",
              "encryption",
              "accessControl",
              "fraudPrevention"
            ]
          }
        }
      ],
      "responses": {
        "200": {
          "description": "Security configurations retrieved successfully",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "items": {
                    "type": "array",
                    "items": {
                      "$ref": "../models/security-authentication.json#/SecurityAuthenticationSettings"
                    }
                  },
                  "total": {
                    "type": "integer"
                  },
                  "page": {
                    "type": "integer"
                  },
                  "limit": {
                    "type": "integer"
                  }
                }
              }
            }
          }
        }
      }
    },
    "post": {
      "tags": ["security-authentication"],
      "summary": "Create a new security configuration",
      "operationId": "createSecurityConfig",
      "requestBody": {
        "required": true,
        "content": {
          "application/json": {
            "schema": {
              "$ref": "../models/security-authentication.json#/SecurityAuthenticationSettings"
            }
          }
        }
      },
      "responses": {
        "201": {
          "description": "Security configuration created successfully",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "../models/security-authentication.json#/SecurityAuthenticationSettings"
              }
            }
          }
        }
      }
    }
  },
  "/security-authentication/{id}": {
    "get": {
      "tags": ["security-authentication"],
      "summary": "Get a specific security configuration",
      "operationId": "getSecurityConfig",
      "parameters": [
        {
          "name": "id",
          "in": "path",
          "required": true,
          "schema": {
            "type": "string",
            "format": "uuid"
          }
        }
      ],
      "responses": {
        "200": {
          "description": "Security configuration retrieved successfully",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "../models/security-authentication.json#/SecurityAuthenticationSettings"
              }
            }
          }
        }
      }
    },
    "put": {
      "tags": ["security-authentication"],
      "summary": "Update a security configuration",
      "operationId": "updateSecurityConfig",
      "parameters": [
        {
          "name": "id",
          "in": "path",
          "required": true,
          "schema": {
            "type": "string",
            "format": "uuid"
          }
        }
      ],
      "requestBody": {
        "required": true,
        "content": {
          "application/json": {
            "schema": {
              "$ref": "../models/security-authentication.json#/SecurityAuthenticationSettings"
            }
          }
        }
      },
      "responses": {
        "200": {
          "description": "Security configuration updated successfully",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "../models/security-authentication.json#/SecurityAuthenticationSettings"
              }
            }
          }
        }
      }
    },
    "delete": {
      "tags": ["security-authentication"],
      "summary": "Delete a security configuration",
      "operationId": "deleteSecurityConfig",
      "parameters": [
        {
          "name": "id",
          "in": "path",
          "required": true,
          "schema": {
            "type": "string",
            "format": "uuid"
          }
        }
      ],
      "responses": {
        "204": {
          "description": "Security configuration deleted successfully"
        }
      }
    }
  }
}
